<?xml version="1.0"?>
<package>
  <name>sr_grasp_msgs</name>
  <version>0.0.1</version>
  <description>Messages, services and actions used by sr_grasp.</description>
  <license>Private</license>
  <url type="repository">https://github.com/shadow-robot/sr-grasp-stabilisation/wiki/sr_grasp_msgs</url>

  <maintainer email="software@shadowrobot.com">Shadow Robot's software team</maintainer>
  <author email="mark@shadowrobot.com">Mark Pitchless</author>
